<div> 
    <main class="m-4">
        <section>
            {{> title data= "Car" big=true}}

            <ul class="mb-3">
                <li>make:{{make}}</li>
                <li>model:{{model}}</li>
                <li>year:{{year}}</li>
                <li>color:{{color}}</li>
            </ul>

            <div>
                <hr>
            </div>
            
            <div>
                <ul>
                    <ul  class="mb-3">
                        {{#with features.interior.seats}}
                        <div>Seats</div>
                        <li>material: {{ material}} </li>
                        <li>color: {{color}} </li>
                        <li>  heated:{{ heated}} </li>
                        <li> ventilated:{{ventilated}} </li>
                        {{/with}}
                    </ul>

                    <div>
                        <hr>
                    </div>

                    <ul  class="mb-3">
                        {{#with features.interior.dashboard}}
                        <div>Dashboard</div>
                        <li> material:{{ material}} </li>
                        <li> color:{{color}} </li>
                        <br>
                            {{#with display}}
                            <div>Display</div>
                                <li>type: {{type}} </li>
                                <li> size:{{size}} </li>
                                <li> resolution:{{resolution}} </li>
                                <li> touchscreen:{{touchscreen}} </li>
                            {{/with}}
                        {{/with}}
                    </ul>
                    <hr>
                    <ul  class="mb-3">
                        {{#with features.interior.audio}}
                        <div>Audio</div>
                            <li> system:{{system}} </li>
                            <li> speakers:{{speakers}} </li>
                            <li> subwoofer:{{subwoofer}} </li>
                            <li> bluetooth:{{bluetooth}} </li>
                            <li> USB:{{USB}} </li>
                        {{/with}}
                    </ul>
                
                     <div>
                         <hr>
                     </div>

                    {{#with features.exterior}}
                        <ul  class="mb-3">
                            {{#with wheels}}
                            <div>Exterior</div>
                                <li> size:{{size}} </li>
                                <li> type:{{type}} </li>
                                <li> color:{{color}} </li>
                            {{/with}}
                        </ul>

                        <div>
                            <hr>
                        </div>

                        <ul  class="mb-3">
                            {{#with headlights}}
                            <div>Headlights</div>
                                <li> type:{{type}} </li>
                                <li> brightness:{{brightness}} </li>
                                <li> automatic:{{automatic}} </li>
                            {{/with}}
                        </ul>
                        <div>
                            <hr>
                        </div>
                        <ul  class="mb-3">
                            {{#with sunroof}}
                            <div>Sunroof</div>
                                <li> type:{{type}} </li>
                                <li> size:{{size}} </li>
                                <li> automatic:{{automatic}} </li>
                            {{/with}}
                        </ul>
                    {{/with}}
                   
                     <div>
                         <hr>
                     </div>

                    {{#with features.safety}}
                        <ul class="mb-3">
                            {{#with airbags}}
                            <div>Airbags</div>
                                <li> front:{{front}} </li>
                                <li> side:{{side}} </li>
                                <li> knee:{{knee}} </li>
                                <li> rear:{{rear}} </li>
                            {{/with}}
                        </ul>

                        <div>
                            <hr>
                        </div>

                         <ul class="mb-3">
                            {{#with assistance}}
                             <div>Assistance</div>
                                <li>Blind spot monitoring:{{blind_spot_monitoring}} </li>
                                <li>Rear cross traffic alert:{{rear_cross_traffic_alert}} </li>
                                <li>Lane departure warning:{{lane_departure_warning}} </li>
                                <li>Adaptive cruise control:{{adaptive_cruise_control}} </li>
                                <li>Collision warning:{{collision_warning}} </li>
                            {{/with}}
                         </ul>
                    {{/with}}

                    <div>
                        <hr>
                    </div>

                </ul>
            </div>
            
                <ul class="mb-3">
                    {{#with engine}}
                    <div>Engine</div>
                        <li> type:{{type}} </li>
                        <li> displacement:{{displacement}} </li>
                        <li> horsepower:{{horsepower}} </li>
                        <li> torque:{{torque}} </li>
                        <ul class="mb-3">
                            {{#with transmission}}
                            <div>Transmission</div>
                                <li> type:{{type}} </li>
                                <li> gears:{{gears}} </li>
                            {{/with}}
                        </ul>
                    {{/with}}
                </ul>

                <div>
                    <hr>
                </div>

                <ul class="mb-3">
                    {{#with fuel_economy}}
                    <div>Fuel economy</div>
                        <li> city:{{city}} </li>
                        <li> highway:{{highway}} </li>
                        <li> combined:{{combined}} </li>
                    {{/with}}
                </ul>
                
                <div>
                    <hr>
                </div>

                 <ul class="mb-3">
                    {{#with price}}
                    <div>Price</div>
                        <li> base:{{base}} </li>
                        <li> destination:{{destination}} </li>
                        <ul>
                            {{#with options}}
                            <div>Options</div class="mb-3">
                                <li>navigation: {{navigation}} </li>
                                <li> moonroof:{{moonroof}} </li>
                                <li>premium_paint: {{premium_paint}} </li>
                            {{/with}}
                        </ul>
                        <p> <b> Total: {{total}} </b> </p>
                    {{/with}}
                 </ul>        
        </section>
    </main>
</div>